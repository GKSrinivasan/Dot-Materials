@model Laserbeam.BusinessObject.Common.RuleConfigModel
@{ string IsSuperAdmin = ""; 
    if (ViewBag.UserRole!="Super Admin") { IsSuperAdmin = "display:none"; }    
}
<link href="~/Content/ManageRules.css" rel="stylesheet" />
<script src="~/Scripts/RuleConfiguration.js"></script>

@if (Model.isWizard == false)
{
    <div class="row">
        <div class="col-md-12 text-right norightpad">
            <button id="btnSaveRule" type="button" class="btn btn-info prim-btn">Save</button>
        </div>
    </div>
}

@if (Model.isWizard == true)
{
    <div class="form-actions">
        <div class="row">
            <div class="col-md-12 text-right top-btn" id="btnLevels">
                <a href="javascript:;" class="btn default button-previous" id="btnBackToWelcome">
                    <i class="fa fa-angle-left"></i> Back
                </a>
                <a href="javascript:;" class="btn btn-outline green button-next" id="btnNextToFields">
                    Save &amp; Continue
                    <i class="fa fa-angle-right"></i>
                </a>

            </div>
        </div>
    </div>
}

<div class="row top-pad">
        <div class="col-sm-3 col-md-3 col-lg-2 noleftpad norightpad wizardleftsec">
            <ul class="nav nav-pills nav-stacked">
                <li class="active"><a id="lnkmanagerulescompensation" href="#tab_a" data-toggle="pill">Compensation Components</a></li>
                <li><a id="lnkmanagerulesgeneralconfig" href="#tab_b" data-toggle="pill">General Configuration</a></li>
                <li><a id="lnkmanagerulesperfratingMeritrule" href="#tab_c" data-toggle="pill">Set Performance rating & Merit rule</a></li>
                <li><a id="lnkmanageruleslumpsumrule" href="#tab_d" data-toggle="pill">Set Lump Sum Rule</a></li>
                <li><a id="lnkmanagerulescollaboration" href="#tab_e" data-toggle="pill">Decision Collaboration</a></li>
                <li><a id="lnkmanagerulesroundingrule" href="#tab_f" data-toggle="pill">Set Rounding Rule</a></li>
                <li><a id="lnkmanagerulesnamedateformat" href="#tab_g" data-toggle="pill">Set Name & Date Format</a></li>
                <li><a id="lnkmanagerulessso" href="#tab_h" data-toggle="pill">Single Sign-on</a></li>
            </ul>
        </div>        
        <div class="col-sm-9 col-md-9 col-lg-10 tabbableright">
        <div class="row">
            <div class="tab-content col-md-12">
                <!--SSO login-->
                <div class="tab-pane" id="tab_h">
                    <div class="table-responsive">
                        <table class="table descriptiontable">
                            <thead>
                                <tr>
                                    <th class="field1 width20">Feature List</th>
                                    <th class="field2 width45"></th>
                                    <th class="field3">What it means?</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="field1 width20">
                                        Enable SSO Login
                                    </td>
                                    <td class="field2">
                                        <div class="register-switch" style="margin:0;">
                                            <input type="radio" name="yesnoSSO" value="Y11" id="chkSSOYes" class="register-switch-input" checked="">
                                            <label for="chkSSOYes" class="register-switch-label"><i class="glyphicon glyphicon-ok"></i></label>
                                            <input type="radio" name="yesnoSSO" value="N11" id="chkSSONo" class="register-switch-input">
                                            <label for="chkSSONo" class="register-switch-label removelabel"><i class="glyphicon glyphicon-remove"></i></label>
                                        </div>
                                    </td>
                                    <td class="field3">
                                       
                                    </td>
                                </tr>
                                <tr>
                                    <td class="field1 width20">
                                        Identifier URL
                                    </td>
                                    <td class="field2 width45">
                                        @*@Model.SSOUrl*@
                                        <div class="input-group" style="width:90%;">
                                            <input class="form-control" id="IdentifierURL" name="URL" readonly="readonly" type="text" value="@Model.SSOUrl">
                                            <a class="input-group-addon whitetxt" onclick="IdentifierTextCopied('IdentifierURL')">
                                                Copy link
                                            </a>
                                        </div>
                                    </td>
                                    <td class="field3">
                                        The URL provided in this section is used to identify the service provider.
                                    </td>
                                </tr>
                                <tr>
                                    <td class="field1 width20">
                                        Reply URL
                                    </td>
                                    <td class="field2 width45">
                                        @*@Model.SSOUrl*@
                                        <div class="input-group" style="width:90%;">
                                            <input class="form-control" id="ReplyURL" name="URL" readonly="readonly" type="text" value="@Model.SSOUrl">
                                            <a class="input-group-addon whitetxt" onclick="ReplyTextCopied('ReplyURL')">
                                                Copy link
                                            </a>
                                        </div>
                                    </td>
                                    <td class="field3">
                                        The URL provided in this section will be the endpoint that accepts SAML response.
                                    </td>
                                </tr>
                                <tr>
                                    <td class="field1 width20">
                                        Single sign-on URL
                                    </td>
                                    <td class="field2 width45">
                                        <input type="text" class="form-control" id="IPE" style="width:90%;" />
                                        <div id="UserStatus"></div>
                                    </td>
                                    <td class="field3">
                                        The URL provided in this section is used by compass to send SAML request.
                                    </td>
                                </tr>                                
                                </tbody>
                            </table>
                        <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
                        </div>
                    </div>
                    <!--End SSO login-->
                    <!--choose increase component-->
                    <div class="tab-pane active" id="tab_a">
                        <div class="row nomargin">
                            <div class="col-md-12">
                                <div class="row top-pad">
                                    <div class="col-md-12 selecttext">
                                        <span class="titlepro1 selectmodule">Select Compensation Components</span>
                                        @*Select what your managers see when making pay decisions*@
                                    </div>
                                </div>
                                <div class="row separation" style="border-bottom:none !important;padding-bottom:20%">
                                    <div class="col-md-12 modulepad">
                                        <div class="tilesec tile bg-yellow-lemon selected" id="divMeritFeature" onclick="selectMeritTile();">
                                            <div style="display:none">
                                                @(Html.Kendo().CheckBox().Name("chkMeritFeature").Label(""))
                                            </div>
                                            <div class="corner"> </div>
                                            <div class="check"> </div>
                                            <div class="tile-body">
                                                <i id="iconMeritFeature" class="icon icon-merit-icon"></i>
                                            </div>
                                            <div class="tile-object">
                                                <div id="lblMeritFeature" class="name"> Merit </div>
                                            </div>
                                        </div>
                                        <div class="tilesec tile bg-yellow-lemon selected" id="divRatingFeature" onclick="selectPerfRatingTile()">
                                            <div style="display:none">
                                                @(Html.Kendo().CheckBox().Name("chkRatingFeature").Label(""))
                                            </div>
                                            <div class="corner"> </div>
                                            <div class="check"> </div>
                                            <div class="tile-body">
                                                <i id="iconRatingFeature" class="icon icon-wiz-02"></i>
                                            </div>
                                            <div class="tile-object">
                                                <div id="lblRatingFeature" class="name"> Performance Rating </div>
                                            </div>
                                        </div>
                                        <div class="tilesec tile bg-yellow-lemon selected" id="divLumpsumFeature" onclick="selectLumpSumTile()">
                                            <div style="display:none">
                                                @(Html.Kendo().CheckBox().Name("chkLumpsumFeature").Label(""))
                                            </div>
                                            <div class="corner"> </div>
                                            <div class="check"> </div>
                                            <div class="tile-body">
                                                <i id="iconLumpsumFeature" class="icon icon-wiz-03"></i>
                                            </div>
                                            <div class="tile-object">
                                                <div id="lblLumpsumFeature" class="name"> Lump Sum </div>
                                            </div>
                                        </div>
                                        <div class="tilesec tile bg-yellow-lemon selected" id="divPromotion" onclick="selectPromotionTile()">
                                            <div style="display:none">
                                                @(Html.Kendo().CheckBox().Name("chkPromotion").Label(""))
                                            </div>
                                            <div class="corner"> </div>
                                            <div class="check"> </div>
                                            <div class="tile-body">
                                                <i id="iconPromotion" class="icon icon-promotion-icon"></i>
                                            </div>
                                            <div class="tile-object">
                                                <div id="lblPromotion" class="name"> Promotion </div>
                                            </div>
                                        </div>
                                        <div class="tilesec tile bg-yellow-lemon selected" id="divAdjustmentFeature" onclick="selectAdjustmentTile()">
                                            <div style="display:none">
                                                @(Html.Kendo().CheckBox().Name("chkAdjustmentFeature").Label(""))
                                            </div>
                                            <div class="corner"> </div>
                                            <div class="check"> </div>
                                            <div class="tile-body">
                                                <i id="iconAdjustmentFeature" class="icon icon-wiz-market-adj"></i>
                                            </div>
                                            <div class="tile-object">
                                                <div id="lblAdjustmentFeature" class="name"> Adjustment </div>
                                            </div>
                                        </div>
                                        <div class="tilesec tile bg-yellow-lemon selected" id="divWorkFlowFeature" onclick="selectWorkFlowTile()">
                                            <div style="display:none">
                                                @(Html.Kendo().CheckBox().Name("chkWorkFlowFeature").Label(""))
                                            </div>
                                            <div class="corner"> </div>
                                            <div class="check"> </div>
                                            <div class="tile-body">
                                                <i id="iconWorkFlowFeature" class="icon-workflow1"></i>
                                                @*<i id="iconWorkFlowFeature" class="icon icon-mm-workflow"></i>*@
                                            </div>
                                            <div class="tile-object">
                                                <div id="lblWorkFlowFeature" class="name"> Workflow </div>
                                            </div>
                                        </div>
                                        <div class="tilesec tile bg-yellow-lemon selected" id="divMultiCurrency" onclick="selectMultiCurrencyTile()">
                                            <div style="display:none">
                                                @(Html.Kendo().CheckBox().Name("chkMultiCurrency").Label(""))
                                            </div>
                                            <div class="corner"> </div>
                                            <div class="check"> </div>
                                            <div class="tile-body">
                                                <i id="iconMultiCurrency" class="icon icon-wiz-multi-currency"></i>
                                            </div>
                                            <div class="tile-object">
                                                <div id="lblMultiCurrency" class="name"> Multi Currency </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-12 modulepad" style="display:none;">
                                        <!--Bonus-->
                                        <div class="tilesec tile bg-yellow-lemon selected" id="divBonus" onclick="selectBonusTile()">
                                            <div style="display:none">
                                                @(Html.Kendo().CheckBox().Name("chkBonus").Label(""))
                                            </div>
                                            <div class="corner"> </div>
                                            <div class="check"> </div>
                                            <div class="tile-body">
                                                <i id="iconBonus" class="icon-rewards-icon"></i>
                                            </div>
                                            <div class="tile-object">
                                                <div id="lblBonus" class="name"> Bonus </div>
                                            </div>
                                        </div>
                                        <!--Bonus End-->
                                    </div>
                                </div>

                            </div>
                        </div>
                        <br /><br /><br />
                    </div>
                    <!--end choose increase component-->
                    <!--general configuration-->
                    <div class="tab-pane" id="tab_b">
                        <div class="table-responsive">
                            <table class="table descriptiontable">
                                <thead>
                                    <tr>
                                        <th class="field1">Feature List</th>
                                        <th class="field2"></th>
                                        <th class="field3">What it means?</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td class="field1">
                                            Current Work Year
                                        </td>
                                        <td class="field2">
                                            <input type="text" class="form-control" id="inpoldyear" style="display:none;" />
                                            <input type="text" class="form-control" id="inpCurrentyear" style="width:60%;" />
                                        </td>
                                        <td class="field3">
                                            The year configured will import data in the system accordingly<br>
                                            <span style="font-style:italic;font-weight:500;">Example:</span> If 2017 is configured, current year will be considered to be 2017 and data prior to 2017 would be considered as history
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="field1">Password Length</td>
                                        <td class="field2 drpdwnfield">
                                            @(Html.Kendo().DropDownList().Name("ddlPasswordLength").HtmlAttributes(new { onChange = "onChange(this.id)" }).
                                                BindTo(Model.PasswordLength.Select(role => new SelectListItem
                                                {
                                                    Text = role,
                                                    Value = role
                                                }).OrderBy(x => Convert.ToInt32(x.Text)).ToArray()).HtmlAttributes(new { style = "width:100%;" })
                                            )
                                        </td>
                                        <td class="field3">Select the length of the password you want to set up for your organization. Use a minimum password length of 4 characters</td>
                                    </tr>
                                    <tr style="@(IsSuperAdmin)">
                                        <td class="field1">Email Address</td>
                                        <td class="field2"><input type="text" class="form-control" id="inpEmailAddress" style="width:60%;" /></td>
                                        <td class="field3">This email is the sender email address for all system generated email notifications sent from the application</td>
                                    </tr>
                                    <div class="noborderline">
                                        <div class="field1">
                                            <div class="row top-pad panel-collapse collapse yet-to" id="takeAct">
                                                <div class="col-sm-12 col-md-12 col-lg-7">
                                                    <p class="s-title">Take Action - Send Reminder Rules</p>
                                                    Send Reminder <input type="text" maxlength="3" size="3" class="tbox" />  days reviews due and every <input type="text" maxlength="3" size="3" class="tbox" /> days after that.  <button id="btnIgnoreRemLogin" type="button" class="btn cancel-btn pull-right" data-toggle="modal" data-target="#takeAction1">Ignore Reminder</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <tr>
                                        <td class="field1">
                                            Currency
                                        </td>
                                        <td class="field2">
                                            <form style="width:60%;">
                                                <label class="radio-inline">
                                                    @(Html.Kendo().RadioButton().Name("radioCurrencyCode").Checked(true).HtmlAttributes(new { @name = "radioButton" }).Label("")) Currency Code
                                                </label><br />
                                                <label class="radio-inline">
                                                    @(Html.Kendo().RadioButton().Name("radioCurrencySymbol").HtmlAttributes(new { @name = "radioButton" }).Label(""))Currency Symbol
                                                </label>
                                            </form>
                                        </td>
                                        <td class="field3">
                                            <span style="font-weight:500;">Currency Code:</span> will be displayed in the budget section <br />
                                            <span style="font-style:italic;font-weight:500;">Example:</span> USD, EUR, INR, AUD, GBP, etc., <span class="clicktext" data-toggle="collapse" data-target="#Currencycode">Click here for Preview</span><br />
                                            <div style="margin-top:7px;"></div>
                                            <span style="font-weight:500;">Currency Symbol:</span> will be displayed in the budget section <br />
                                            <span style="font-style:italic;font-weight:500;">Example:</span> $, € <span class="clicktext" data-toggle="collapse" data-target="#Currencysymbol">Click here for Preview</span>
                                        </td>
                                    </tr>
                                    <tr class="collapse" id="Currencycode">
                                        <td>
                                            <!--preview currency code-->
                                            <div>
                                                <div class="col-sm-12 col-md-12 col-lg-12 top-pad titlepro" style="margin-top:15px;">
                                                    <button id="btnsetrulescloseCurrencycode" type="button" class="close" data-toggle="collapse" data-target="#Currencycode">×</button>
                                                    <img src="~/Images/currencycode.png" class="img-responsive" />
                                                </div>
                                            </div>
                                            <!--end preview currency code-->
                                        </td>
                                    </tr>
                                    <tr class="collapse" id="Currencysymbol">
                                        <td>
                                            <!--preview currency code-->
                                            <div>
                                                <div class="col-sm-12 col-md-12 col-lg-12 top-pad titlepro" style="margin-top:15px;">
                                                    <button id="btnsetrulescloseCurrencysymbol" type="button" class="close" data-toggle="collapse" data-target="#Currencysymbol">×</button>
                                                    <img src="~/Images/currencysymbol.png" class="img-responsive" />
                                                </div>
                                            </div>
                                            <!--end preview currency code-->
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="field1">
                                            Default budget currency format
                                        </td>
                                        <td class="field2 bigfield">
                                            <div class="register-switch">
                                                <input type="radio" name="yesno6" value="Y6" id="BaseCurrency" class="register-switch-input" checked="">
                                                <label for="BaseCurrency" class="register-switch-label">Logged User Currency</label>
                                                <input type="radio" name="yesno6" value="N6" id="OtherCurrency" class="register-switch-input">
                                                <label for="OtherCurrency" class="register-switch-label">Base Currency</label>
                                            </div>
                                        </td>
                                        <td class="field3"></td>
                                    </tr>

                                    <tr>
                                        <td class="field1">
                                            Clear workflow action performed in recommend increase section
                                        </td>
                                        <td class="field2 bigfield">
                                            <div class="text-left"><button type="button" id="btnExecute" class="btn btn-cancel">Clear workflow process</button></div>
                                        </td>
                                        <td class="field3">
                                            This will clear out the workflow actions that you have performed in the Rewards >> Recommend Increase section. You cannot retrieve this data if you execute clear script.
                                        </td>
                                    </tr>

                                    <tr style="display:none;">
                                        <td class="field1">Individual Portion</td>
                                        <td class="field2"><input type="text" class="form-control" id="inpBonusIndividual" style="width:60%;" /></td>
                                        <td class="field3"></td>
                                    </tr>
                                    <tr style="display:none;">
                                        <td class="field1">Global Portion</td>
                                        <td class="field2"><input type="text" class="form-control" id="inpBonusGlobal" style="width:60%;" /></td>
                                        <td class="field3"></td>
                                    </tr>

                                </tbody>
                            </table>
                        </div>
                    </div>
                    <!--end general configuration-->
                    <!--set merit proration-->
                    <div class="tab-pane" id="tab_c">

                        <!--new model-->


                        <div class="table-responsive" id="fldProrationRule">
                            <table class="table descriptiontable">
                                <thead>
                                    <tr>
                                        <th class="field1">Feature List</th>
                                        <th class="field2"></th>
                                        <th class="field3">What it means?</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr class="noborderline">
                                        <td class="field1" style="color:#06b3a2;font-size:17px; padding-top:15px; font-style:italic;">Comparative Ratio</td>
                                        <td class="field2"></td>
                                        <td class="field3"></td>
                                    </tr>
                                    <tr>
                                        <td class="field1">Auto calculate both current and new compa-ratio using single salary range data</td>
                                        <td class="field2">
                                            <div class="register-switch">
                                                <input type="radio" name="yesno0" value="Y0" id="CompaRatioDropdownYes" class="register-switch-input" checked="">
                                                <label for="CompaRatioDropdownYes" class="register-switch-label"><i class="glyphicon glyphicon-ok"></i></label>
                                                <input type="radio" name="yesno0" value="N0" id="CompaRatioDropdownNo" class="register-switch-input">
                                                <label for="CompaRatioDropdownNo" class="register-switch-label removelabel"><i class="glyphicon glyphicon-remove"></i></label>
                                            </div>
                                        </td>
                                        <td class="field3">This option would auto calculate both current and new compa-ratio using single salary range data. Turn it off if the current and new compa-ratio is calculated using two different salary range data. In this case you would upload the current compa-ratio. The tool will calculate the new compa-ratio.</td>
                                    </tr>
                                    <tr class="noborderline">
                                        <td class="field1" style="color:#06b3a2;font-size:17px; padding-top:15px; font-style:italic;">Performance Rating</td>
                                        <td class="field2"></td>
                                        <td class="field3"></td>
                                    </tr>
                                    <tr>
                                        <td class="field1">Allow manager to select performance rating</td>
                                        <td class="field2">
                                            <div class="register-switch">
                                                <input type="radio" name="yesno" value="Y" id="RatingDropdownYes" class="register-switch-input" checked="">
                                                <label for="RatingDropdownYes" class="register-switch-label"><i class="glyphicon glyphicon-ok"></i></label>
                                                <input type="radio" name="yesno" value="N" id="RatingDropdownNo" class="register-switch-input">
                                                <label for="RatingDropdownNo" class="register-switch-label removelabel"><i class="glyphicon glyphicon-remove"></i></label>
                                            </div>
                                        </td>
                                        <td class="field3">This option allows managers to modify the Performance Ratings uploaded into the tool.</td>
                                    </tr>
                                    <tr class="noborderline">
                                        <td class="field1" style="color:#06b3a2;font-size:17px; padding-top:15px; font-style:italic;">Merit</td>
                                        <td class="field2"></td>
                                        <td class="field3"></td>
                                    </tr>
                                    <tr>
                                        <td class="field1">
                                            Total Cash Compensation (TCC)
                                        </td>
                                        <td class="field2">
                                            <div class="register-switch">
                                                <input type="radio" name="yesno1" value="Y1" id="chkTCCYes" class="register-switch-input" checked="">
                                                <label for="chkTCCYes" class="register-switch-label"><i class="glyphicon glyphicon-ok"></i></label>
                                                <input type="radio" name="yesno1" value="N1" id="chkTCCNo" class="register-switch-input">
                                                <label for="chkTCCNo" class="register-switch-label removelabel"><i class="glyphicon glyphicon-remove"></i></label>
                                            </div>
                                        </td>
                                        <td class="field3">TCC column will be shown on Recommend increase page</td>
                                    </tr>
                                    <tr>
                                        <td class="field1">Auto calculate the employee salary to the market minimum range</td>
                                        <td class="field2">
                                            <div class="register-switch">
                                                <input type="radio" name="yesno2" value="Y2" id="meritCalcYes" class="register-switch-input" checked="">
                                                <label for="meritCalcYes" class="register-switch-label"><i class="glyphicon glyphicon-ok"></i></label>
                                                <input type="radio" name="yesno2" value="N2" id="meritCalcNo" class="register-switch-input">
                                                <label for="meritCalcNo" class="register-switch-label removelabel"><i class="glyphicon glyphicon-remove"></i></label>
                                            </div>
                                        </td>
                                        <td class="field3">
